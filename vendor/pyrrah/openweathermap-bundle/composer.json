{
    "name": "pyrrah/openweathermap-bundle",
    "description": "The simple way to obtain currently weather from OpenWeatherMap API, for Symfony 5 to 6.",
    "homepage": "https://github.com/Pyrrah/OpenWeatherMapBundle",
    "keywords": ["pyrrah", "openweathermap", "open", "weather", "map", "weathermap", "weathermap symfony5", "weathermap symfony6"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre-Yves Dick",
            "email": "hello@pierreyvesdick.fr",
            "homepage": "https://github.com/Pyrrah"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.5",
        "guzzlehttp/guzzle": "^7.5"
    },
    "autoload": {
        "psr-4": { "Pyrrah\\OpenWeatherMapBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Pyrrah\\OpenWeatherMapBundle\\Tests\\": "tests/" }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "v1.1.x-dev"
        }
    }
}
